[dataset]
dataset_name = "QM9"
version_select = "full_dataset_v1_HCNOFClS"
num_workers = 4
pin_memory = true
properties_of_interest = ["atomic_numbers", "geometry", "dft_total_energy", "dft_total_force", "total_charge", "scf_dipole"]
element_filter = [[-16, -17]]   # H C N O F, remove Cl, S

[dataset.properties_assignment]
atomic_numbers = "atomic_numbers"
positions = "geometry"
E = "dft_total_energy"
F = "dft_total_force"
total_charge = "total_charge"
dipole_moment = "scf_dipole"
[dataset]
dataset_name = "tmqm"
version_select = "latest"
num_workers = 4
pin_memory = true
properties_of_interest = ["atomic_numbers", "geometry", "total_energy", "total_charge", "dipole_moment_computed"]
element_filter = {{ element_filter }}

[dataset.properties_assignment]
atomic_numbers = "atomic_numbers"
positions = "geometry"
E = "total_energy"
total_charge = "total_charge"
dipole_moment = "dipole_moment_computed"